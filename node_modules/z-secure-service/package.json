  "license": "ISC",
  "description": "A rate-limiting and API protection middleware that helps developers add security features to their APIs effortlessly.",
  "homepage": "https://github.com/LEVIII007/z-secure-SDK.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/LEVIII007/z-secure-SDK.git"
  },
  "bugs": {
    "url": "https://github.com/LEVIII007/z-secure-SDK.git/issues"
  },
  "dependencies": {
