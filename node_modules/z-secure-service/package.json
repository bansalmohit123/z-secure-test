  "scripts": {
    "build": "tsc && mv dist/* ./ && rmdir dist",
    "start": "node dist/index.js",
    "dev": "nodemon --watch src --exec ts-node src/index.ts",
    "test": "jest"
  },
  "type": "module",
  "files": [
    "LICENSE",
    "README.md",
    "*.js",
    "*.d.ts",
    "!*.config.js"
